new rl(`rho:registry:lookup`), PoSCh in {
  rl!(`rho:rchain:pos`, *PoSCh) |
  for(@(_, PoS) <- PoSCh){
    new deployerId(`rho:rchain:deployerId`), payAck in {
      @PoS!("chargeDeploy", 100, *deployerId, *payAck) |
      for(@(true, _) <- payAck){
        new closeBlockAck in {
          @PoS!("closeBlock", *closeBlockAck)
        }
      }
    }
  }
}