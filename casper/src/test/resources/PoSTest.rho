// Match a list of known valid publicKey -> RevAddress pairs to bind them to veriables
new
  rl(`rho:registry:lookup`), RhoSpecCh,
  PosCh,
  setup,
  test, test_different_deployer,
  ackCh
in {
  rl!(`rho:id:6wnujzcraztjfg941skrtbdkdgbko8nuaqihuhn15s66oz8ro5gwbb`, *RhoSpecCh) |
  rl!(`rho:id:cnec3pa8prp4out3yc8facon6grm3xbsotpd4ckjfx8ghuw77xadzt`, *PosCh) |
  for(@(_, RhoSpec) <- RhoSpecCh; @(_, Pos) <- PosCh) {
    @RhoSpec!("testSuite", *setup,
      [
        ("work", *test),
      ]) |

    contract setup(returnCh) = {
      returnCh!(Nil)
    } |

    contract test(rhoSpec, _, ackCh) = {
      rhoSpec!("assert", (Nil, "!=", Pos), "ther is PoS", *ackCh)
    }
  }
}
