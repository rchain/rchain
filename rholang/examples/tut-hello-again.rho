new helloAgain, stdout(`rho:io:stdout`), stdoutAck(`rho:io:stdoutAck`) in {
  contract helloAgain(_) = {
    new chan in {
      chan!("Hello again, world!") |
      for (@text <- chan) {
        new ack in {
          stdoutAck!(text, *ack) |
          for (_ <- ack) {
            stdout!("\n")
          }
        }
      }
    }
  } |
  helloAgain!(Nil) |
  helloAgain!(Nil)
}
